                                                                                                                                   
   {                                                                                                                                  
    "Feature": "Autolytics",                                                                                                         
    "Description": "This feature covers the functionality of the Autolytics screen in the Jewel ecosystem",                          
    "Scenarios": [                                                                                                                   
      {                                                                                                                              
        "Scenario": "Verify Link Change after Clicking to Autolytics button",                                                        
        "Steps": [                                                                                                                   
          {                                                                                                                          
            "StepType": "Given",                                                                                                     
            "Description": "navigate to login-screen",                                                                               
            "xpath": "//a[contains(@href, 'login') or contains(text(), 'Login')]"                                                    
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "login to jewel website",                                                                                 
            "xpath": "//form[@id='login-form' or contains(@class, 'login-form')]"                                                    
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "When",                                                                                                      
            "Description": "Click on the Autolytics screen",                                                                         
            "xpath": "//a[contains(text(), 'Autolytics') or contains(@href, 'autolytics')]"                                          
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "the page link should change",                                                                            
            "xpath": "//script[contains(text(), 'window.location.href')]"                                                            
          }                                                                                                                          
        ]                                                                                                                            
      },                                                                                                                             
      {                                                                                                                              
        "Scenario": "Request Access to Projects in Autolytics",                                                                      
        "Steps": [                                                                                                                   
          {                                                                                                                          
            "StepType": "Given",                                                                                                     
            "Description": "user is on Autolytics screen",                                                                           
            "xpath": "//div[contains(@class, 'autolytics-screen') or contains(@id, 'autolytics')]"                                   
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "there are no projects available",                                                                        
            "xpath": "//div[contains(text(), 'No projects available') or contains(@class, 'no-projects')]"                           
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "When",                                                                                                      
            "Description": "the user clicks on the Request Access button",                                                           
            "xpath": "//button[contains(text(), 'Request Access') or @id='request-access-btn']"                                      
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "the user should be navigated to the Admin tab and create a new project",                                 
            "xpath": "//div[contains(@class, 'admin-tab') or @id='admin-tab']//button[contains(text(), 'Create New Project')]"       
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "verify the project is created",                                                                          
            "xpath": "//div[contains(@class, 'project-list')]//div[contains(@class, 'project-item')]"                                
          }                                                                                                                          
        ]                                                                                                                            
      },                                                                                                                             
      {                                                                                                                              
        "Scenario": "Verify Total Number of Projects Displayed in Autolytics",                                                       
        "Steps": [                                                                                                                   
          {                                                                                                                          
            "StepType": "Given",                                                                                                     
            "Description": "navigate to Autolytics screen",                                                                          
            "xpath": "//a[contains(text(), 'Autolytics') or contains(@href, 'autolytics')]"                                          
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "When",                                                                                                      
            "Description": "user look at the "Total projects " column",                                                              
            "xpath": "//th[contains(text(), 'Total projects')]"                                                                      
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "user should see the expected total number of projects as "1"",                                           
            "xpath": "//td[contains(@class, 'total-projects') and text()='1']"                                                       
          }                                                                                                                          
        ]                                                                                                                            
      },                                                                                                                             
      {                                                                                                                              
        "Scenario": "Verify Active Users Count",                                                                                     
