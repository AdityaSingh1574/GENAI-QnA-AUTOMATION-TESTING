          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "the name of the suite should appear under the Latest Run column",                                        
            "xpath": "//th[contains(text(), 'Latest Run')]//following-sibling::td[contains(text(), 'suiteName')]"                    
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "the suite name in Autolytics should match suiteName",                                                    
            "xpath": "//td[contains(@class, 'suite-name') and text()='suiteName']"                                                   
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "the project name in Autolytics should match projectName",                                                
            "xpath": "//td[contains(@class, 'project-name') and text()='projectName']"                                               
          }                                                                                                                          
        ]                                                                                                                            
      },                                                                                                                             
