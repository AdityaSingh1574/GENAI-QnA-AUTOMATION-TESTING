      {                                                                                                                              
        "Scenario": "Verify Manual TestCase Count",                                                                                  
        "Steps": [                                                                                                                   
          {                                                                                                                          
            "StepType": "Given",                                                                                                     
            "Description": "user is on Autolytics screen",                                                                           
            "xpath": "//div[contains(@class, 'autolytics-screen') or contains(@id, 'autolytics')]"                                   
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "When",                                                                                                      
            "Description": "user look at the Manual TestCase column",                                                                
            "xpath": "//th[contains(text(), 'Manual TestCase')]"                                                                     
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "user should see the expected number of Manual testcases as "0"",                                         
            "xpath": "//td[contains(@class, 'manual-testcases') and text()='0']"                                                     
          }                                                                                                                          
        ]                                                                                                                            
      },                                                                                                                             
      {                                                                                                                              
        "Scenario": "Verify the Increase in Pass and Fail Scenarios",                                                                
        "Steps": [                                                                                                                   
          {                                                                                                                          
            "StepType": "Given",                                                                                                     
            "Description": "user is on Autolytics screen",                                                                           
            "xpath": "//div[contains(@class, 'autolytics-screen') or contains(@id, 'autolytics')]"                                   
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "When",                                                                                                      
            "Description": "user click on MLab to create testcases",                                                                 
            "xpath": "//a[contains(text(), 'MLab') or contains(@href, 'mlab')]"                                                      
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user create a folder named folderName in the MLab",                                                      
            "xpath": "//button[contains(text(), 'Create Folder') or @id='create-folder-btn']"                                        
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user add a manual test case named testCaseName to the folder",                                           
            "xpath": "//button[contains(text(), 'Add Test Case') or @id='add-testcase-btn']"                                         
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user add test steps for testCaseName named testStepName",                                                
            "xpath": "//button[contains(text(), 'Add Test Step') or @id='add-teststep-btn']"                                         
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user go to ELab",                                                                                        
            "xpath": "//a[contains(text(), 'ELab') or contains(@href, 'elab')]"                                                      
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user create a suite named suiteName in ELab",                                                            
            "xpath": "//button[contains(text(), 'Create Suite') or @id='create-suite-btn']"                                          
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user import and add the testcases to the suite",                                                         
            "xpath": "//button[contains(text(), 'Import Test Cases') or @id='import-testcases-btn']"                                 
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user execute the suite",                                                                                 
            "xpath": "//button[contains(text(), 'Execute Suite') or @id='execute-suite-btn']"                                        
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "the report link should be available",                                                                    
            "xpath": "//a[contains(@class, 'report-link') or contains(@href, 'report')]"                                             
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user click on the report link",                                                                          
            "xpath": "//a[contains(@class, 'report-link') or contains(@href, 'report')]"                                             
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user assign testCaseOutcomes to the test steps",                                                         
            "xpath": "//select[contains(@class, 'test-outcome') or @id='test-outcome-select']"                                       
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "user save the changes",                                                                                  
            "xpath": "//button[contains(text(), 'Save Changes') or @id='save-changes-btn']"                                          
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "Click on the Autolytics screen",                                                                         
            "xpath": "//a[contains(text(), 'Autolytics') or contains(@href, 'autolytics')]"                                          
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "the number of "FAIL" test cases should increase by "1"",                                                 
            "xpath": "//td[contains(@class, 'fail-testcases') and text()='1']"                                                       
          }                                                                                                                          
        ]                                                                                                                            
      },                                                                                                                             
      {                                                                                                                              
        "Scenario": "Verify Suite Name in Latest Run and Project Name Matches",                                                      
        "Steps": [                                                                                                                   
          {                                                                                                                          
            "StepType": "Given",                                                                                                     
            "Description": "user is on Autolytics screen",                                                                           
