    "Scenarios": [                                                                                                                   
      {                                                                                                                              
        "Scenario": "Validate count on assigning learner",                                                                           
        "Steps": [                                                                                                                   
          {                                                                                                                          
            "StepType": "Given",                                                                                                     
            "Description": "Select "Manage Courses", "Batches" from sidebar",                                                        
            "xpath": "//ul[@class='sidebar-menu']//a[contains(text(), 'Manage Courses')]/..//a[contains(text(), 'Batches')]"         
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "When",                                                                                                      
            "Description": "Click actions icon of a batch",                                                                          
            "xpath": "//table[@id='batches-table']//tr[1]//button[@class='actions-icon' or contains(@class, 'dropdown-toggle')]"     
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "Select "Assign Learners" from actions dropdown",                                                         
            "xpath": "//ul[contains(@class, 'dropdown-menu')]//a[text()='Assign Learners']"                                          
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "Get assigned learners count",                                                                            
            "xpath": "//div[contains(@class, 'assigned-learners-count')]"                                                            
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "And",                                                                                                       
            "Description": "Assign a learner",                                                                                       
            "xpath": "//table[@id='learners-table']//tr[1]//button[contains(@class, 'assign-learner')]"                              
          },                                                                                                                         
          {                                                                                                                          
            "StepType": "Then",                                                                                                      
            "Description": "Validate count after assigning",                                                                         
            "xpath": "//div[contains(@class, 'assigned-learners-count')]"                                                            
          }                                                                                                                          
        ]                                                                                                                            
      },                                                                                                                             
